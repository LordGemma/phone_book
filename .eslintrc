{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "no-useless-rename": ["error", {
          "ignoreDestructuring": false,
          "ignoreImport": false,
          "ignoreExport": false
        }],
        "import/prefer-default-export": "off"
      }
}